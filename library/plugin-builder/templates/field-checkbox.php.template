
/**
 * Render {{field_id}} field.
 */
function {{prefix}}_render_{{field_id}}_field(): void {
	$options = get_option( '{{prefix}}_settings' );
	$checked = isset( $options['{{field_id}}'] ) ? checked( $options['{{field_id}}'], 1, false ) : '';
	echo '<label><input type="checkbox" name="{{prefix}}_settings[{{field_id}}]" value="1" ' . $checked . '>';
	esc_html_e( 'Enable', '{{prefix}}' );
	echo '</label>';{{desc_html}}
}
